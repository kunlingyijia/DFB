#import <UIKit/UIKit.h>

@interface AddressManageCell : UITableViewCell

@property (weak, nonatomic) IBOutlet UILabel *name;      //名字
@property (weak, nonatomic) IBOutlet UILabel *phone;     //手机号1
@property (weak, nonatomic) IBOutlet UILabel *address;   //地址

@property (weak, nonatomic) IBOutlet UIButton *editBtn;      //"编辑"按钮
